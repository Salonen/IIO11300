        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Height="16" Margin="224,54,0,0" TextWrapping="Wrap" Text="Valmistettu" VerticalAlignment="Top" Width="62"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="100" Margin="22,75,0,0" VerticalAlignment="Top" Width="285"/>
        

    </Grid>
